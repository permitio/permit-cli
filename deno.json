{
	"tasks": {
		"build": "deno compile --allow-read --allow-write --allow-net --allow-env --allow-run --allow-sys --unstable-sloppy-imports --no-check --output permit-deno --include source/commands source/cli-deno.tsx",
		"build:node": "npm run build",
		"test": "deno test --allow-read --allow-write --allow-net --allow-env --allow-run --allow-sys",
		"lint": "deno lint",
		"fmt": "deno fmt"
	},
	"imports": {
		"path": "node:path",
		"fs": "node:fs",
		"url": "node:url",
		"util": "node:util",
		"os": "node:os",
		"crypto": "node:crypto",
		"stream": "node:stream",
		"buffer": "node:buffer",
		"events": "node:events",
		"react": "npm:react@18.2.0",
		"react/jsx-runtime": "npm:react@18.2.0/jsx-runtime",
		"react/jsx-dev-runtime": "npm:react@18.2.0/jsx-dev-runtime",
		"react-dom": "npm:react-dom@18.2.0",
		"ink": "npm:ink@4.2.0",
		"ink-gradient": "npm:ink-gradient@3.0.0",
		"ink-spinner": "npm:ink-spinner@5.0.0",
		"@inkjs/ui": "npm:@inkjs/ui@1.0.0",
		"@scaleway/random-name": "npm:@scaleway/random-name@1.2.0",
		"open": "npm:open@9.1.0",
		"openapi-fetch": "npm:openapi-fetch@0.13.4",
		"openapi-typescript-helpers": "npm:openapi-typescript-helpers@0.0.15",
		"commander": "npm:commander@11.1.0",
		"pastel": "npm:pastel@3.0.0",
		"zod": "npm:zod@3.22.4",
		"./source/lib/api/v1.js": "./source/lib/api/v1.d.ts",
		"./source/lib/api/pdp-v1.js": "./source/lib/api/pdp-v1.d.ts"
	},
	"compilerOptions": {
		"lib": ["deno.window", "dom", "dom.iterable", "dom.asynciterable", "deno.ns"],
		"strict": true,
		"jsx": "react-jsx",
		"jsxImportSource": "react",
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"module": "esnext",
		"moduleResolution": "node",
		"resolveJsonModule": true,
		"isolatedModules": true,
		"noEmit": true,
		"allowJs": true,
		"checkJs": false
	},
	"nodeModulesDir": "auto"
}
